    "axios": "^0.19.0",
    "babelify": "^10.0.0",
    "connect-flash": "^0.1.1",
    "connect-mongo": "^3.0.0",
    "cookie-parser": "^1.4.4",
    "core-js": "^3.3.4",
    "cross-env": "^6.0.3",
    "del": "^5.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.16.2",
