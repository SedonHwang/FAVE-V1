  "description": "fave webpage for Strong Friend By RunningWater ",
  "main": "build/init.js",
  "scripts": {
    "dev:server": "nodemon --exec babel-node src/init.js --delay 2",
    "dev:assets": "gulp dev",
    "build:server": "babel src --out-dir build",
